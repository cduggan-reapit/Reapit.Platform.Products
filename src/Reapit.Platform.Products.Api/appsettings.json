{
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [{
      "Name": "Console",
      "Args": {
        "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
      }
    }]
  },
  "RemoteConfigurationParameter": "/Platform/Products/Configuration"
}
